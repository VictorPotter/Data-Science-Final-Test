SET DATESTYLE TO PostgreSQL,European;

--------------------------Criação--------------------------------------

CREATE SEQUENCE IDCliente;
CREATE TABLE Cliente(
	IDCliente int default nextval('IDCliente'::regclass) PRIMARY KEY NOT NULL,
	Nome Varchar(50),
	Estado Varchar(2),
	Sexo char(1)
);

CREATE SEQUENCE IDCredito;
CREATE TABLE Credito(
 	IDCredito int PRIMARY KEY NOT NULL,
 	"Duração" integer,
	"HistoricoCredito" integer,
    "Proposito" integer,
    "Valor" integer,
    "Investimentos" integer,
    "Emprego" integer,
    "TempoParcelamento" integer,
    "EstadoCivil" integer,
    "FiadorTerceiros" integer,
    "ResidenciaDesde" integer,
    "Idade" integer,
    "OutrosFinanciamentos" integer,
    "Habitacao" integer,
    "EmprestimoExistente" integer,
    "Profissao" integer,
    "Dependentes" integer,
    "SocioEmpresa" integer,
    "Estrangeiro" integer,
    "Status" integer
);

CREATE SEQUENCE IDEmprego;
CREATE TABLE Emprego(
	IDEmprego int PRIMARY KEY NOT NULL,
	Emprego Varchar(50) NOT NULL
);

CREATE SEQUENCE IDEstadoCivil;
CREATE TABLE EstadoCivil(
	IDEstadoCivil int PRIMARY KEY NOT NULL,
	EstadoCivil Varchar(20) NOT NULL
);

CREATE SEQUENCE IDFiador;
CREATE TABLE Fiador(
	IDFiador int PRIMARY KEY NOT NULL,
	Fiador Varchar(50) NOT NULL
);



CREATE SEQUENCE IDHabitacao;
CREATE TABLE Habitacao(
	IDHabitacao int PRIMARY KEY NOT NULL,
	Habitacao Varchar(50) NOT NULL
);

CREATE SEQUENCE IDHistorico_Credito;
CREATE TABLE Historico_Credito(
	IDHistorico_Credito int PRIMARY KEY NOT NULL,
	Historico_Credito Varchar(100) NOT NULL
);

CREATE SEQUENCE IDInvestimentos;
CREATE TABLE Investimentos(
	IDInvestimentos int PRIMARY KEY NOT NULL,
	Investimentos Varchar(100) NOT NULL
);

CREATE SEQUENCE IDOutrosFinanc;
CREATE TABLE OutrosFinanc(
	IDOutrosFinanc int PRIMARY KEY NOT NULL,
	OutrosFinanc Varchar(100) NOT NULL
);

CREATE SEQUENCE IDProfissao;
CREATE TABLE Profissao(
	IDProfissao int PRIMARY KEY NOT NULL,
	Profissao Varchar(50) NOT NULL
);

CREATE SEQUENCE IDProposito;
CREATE TABLE Proposito(
	IDProposito int PRIMARY KEY NOT NULL,
	Proposito Varchar(100) NOT NULL
);

-------------------------Inserção de Dados--------------------------

COPY cliente(IDCliente, Nome, Estado, Sexo) FROM stdin;

